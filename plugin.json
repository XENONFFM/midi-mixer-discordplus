{
  "$schema": "./node_modules/midi-mixer-plugin/plugin.schema.json",
  "id": "com.midi-mixer.discordplus",
  "name": "Discord+",
  "type": "node",
  "version": "0.1",
  "author": "https://github.com/XENONFFM",
  "main": "lib/main.js",
  "icon": "icon.svg",
  "settings": {
    "clientId": {
      "label": "Client ID",
      "required": true,
      "type": "text"
    },
    "clientSecret": {
      "label": "Client Secret",
      "required": true,
      "type": "password"
    },
    "status": {
      "label": "Status",
      "type": "status",
      "fallback": "Disconnected"
    },
    "reconnect": {
      "label": "Reconnect",
      "type": "button"
    },
    "userCount": {
      "label": "Number of VoiceUsers",
      "type": "text",
      "fallback": "8"
    }
  }
}